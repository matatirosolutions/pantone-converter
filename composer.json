        "HEX",
        "RGB",
        "CMYK"
    ],
    "authors": [
        {
            "name": "Steve Winter",
            "email": "steve@msdev.nz"
        }
    ],
    "type": "library",
    "license": "MIT",
    "support": {
        "email": "support@msdev.nz",
        "issues": "https://github.com/matatirosolutions/pantone-converter/issues"
    },
    "require": {
        "php": ">=7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3"
    },
    "config": {
